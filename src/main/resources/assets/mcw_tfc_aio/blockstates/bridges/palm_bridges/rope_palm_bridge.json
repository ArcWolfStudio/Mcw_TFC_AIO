{
  "multipart": [

    {   "when": { "OR": [
        { "facing": "north|south"}
        ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_base", "y": 90}
    },
	{   "when": { "OR": [
        { "facing": "east|west"}
        ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_base", "y": 0}
    },
	
        {   "when": { "torch": "true" },
            "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_base_torch" }
        },
	
	
	{
      "when": { "AND": [  {"facing": "east|west","toggle": "true","north": "true","east": "false","south": "false","west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_right", "y": 0}
    },
	{
      "when": { "AND": [  {"facing": "east|west","toggle": "true","north": "false","east": "false","south": "true","west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_left", "y": 180}
    },
	{
      "when": { "AND": [  {"facing": "east|west","toggle": "true","north": "false","east": "true","south": "true","west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_right", "y": 180}
    },
	{
      "when": { "AND": [  {"facing": "east|west","toggle": "true","north": "true","east": "true","south": "false","west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_left", "y": 0}
    },

	{
      "when": { "AND": [  {"facing": "north|south","toggle": "true","north": "true","east": "false","south": "false","west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_left", "y": 270}
    },
	{
      "when": { "AND": [  {"facing": "north|south","toggle": "true","north": "false","east": "false","south": "true","west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_right", "y": 270}
    },
	{
      "when": { "AND": [  {"facing": "north|south","toggle": "true","north": "false","east": "true","south": "true","west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_left", "y": 90}
    },
	{
      "when": { "AND": [  {"facing": "north|south","toggle": "true","north": "true","east": "true","south": "false","west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side_right", "y": 90}
    },
    {
      "when": { "AND": [  {"toggle": "false","north": "true","west": "true","east": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_corner_clear", "y": 180}
    },
	{
      "when": { "AND": [  {"toggle": "false","north": "true","east": "true","west": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_corner_clear", "y": 270}
    },
	{
      "when": { "AND": [  {"toggle": "false","south": "true","east": "true","west": "false","north": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_corner_clear"}
    },
	{
      "when": { "AND": [  {"toggle": "false","south": "true","west": "true","east": "false","north": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_corner_clear", "y": 90}
    },
		
	{
      "when": { "AND": [  {"toggle": "false","north": "true", "east": "true","south": "false", "west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side"}
    },		
	{
      "when": { "AND": [  {"toggle": "false","north": "false", "east": "true","south": "true", "west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side", "y": 180}
    },	
	{
      "when": { "AND": [  {"toggle": "false","north": "true", "east": "true","south": "true", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side", "y": 90}
    },	
	{
      "when": { "AND": [  {"toggle": "false","north": "true", "east": "false","south": "true", "west": "true"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_side", "y": 270}
    },	
		
    {
      "when": { "AND": [  {"north": "false", "south": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle_end", "y": 180}
    },
    {
      "when": { "AND": [  {"south": "false","north": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle_end"}
    },
    {
      "when": { "AND": [  {"east": "false","west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle_end", "y": 270}
    },
  	{
      "when": { "AND": [  {"west": "false","east": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle_end", "y": 90}
    },
	{
      "when": { "AND": [  {"east": "true", "west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle", "y": 90}
    },
	{
      "when": { "AND": [  {"south": "true", "north": "true", "east": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle"}
    },
	{
      "when": { "AND": [  {"facing": "north|south","south": "false", "east": "false", "north": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle", "y": 90}
    },
	{
      "when": { "AND": [  {"facing": "east|west","south": "false", "east": "false", "north": "false", "west": "false"} ]},
      "apply": { "model": "mcw_tfc_aio:block/bridge/rope/palm/palm_middle"}
    }
  ]
}	
